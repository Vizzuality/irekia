<ul id="users">
  <li>
    <%= link_to t('.new_user'), new_admin_user_path %>
  </li>
  <% @users.each do |user| %>
  <li class="user">
  <%= link_to user.name, edit_admin_user_path(user) %>
  &middot;
  <%= link_to t('.edit_user'), edit_admin_user_path(user) %>
  |
  <%= link_to t('.delete_user'), admin_user_path(user), :method => :delete %>
  </li>
  <% end %>
</ul>