<h4><%= t('.title') %></h4>
<ul class="people no-border no-padding extra-margin-bottom">
  <% @content.users.each do |politician| -%>
  <li>
    <%= avatar politician, 'small' %>
    <%= link_to politician.fullname, path_for_user(politician), :class => :name %>
    <span class="position"><%= get_politician_title(politician) %></span>
  </li>
  <% end -%>
</ul>
