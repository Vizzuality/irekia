<% if @show_title -%>
  <h2><%= t(".results.title.#{params[:type].pluralize || 'total'}", :count => @contents_count[:"#{params[:type].try(:pluralize) || 'total'}"]) %></h2>
<% end -%>
<ul>
  <% if @contents.size > 0 %>
    <% @contents.each do |content| %>
      <li class="<%= content.event_type %>">
      <%= render "shared/lists_elements/#{content.event_type}", :data => content.item %>
      </li>
    <% end %>
  <% else %>
    <li>&nbsp;</li>
  <% end %>
</ul>
